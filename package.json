{
  "name": "coq-to-ocaml-to-js",
  "version": "0.1.0",
  "repository": "https://github.com/imbsky/coq-to-ocaml-to-js",
  "author": "BSKY <me@bsky.moe>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "size": "bundlesize",
    "coq:clean": "rimraf --glob ocaml/*.{ml,mli}",
    "bsb:clean": "bsb -clean-world",
    "bsb:build": "bsb -make-world",
    "rollup:clean": "rimraf --glob javascript/*.js",
    "rollup:build": "rollup --config rollup.config.ts",
    "clean": "npm-run-all -p *:clean",
    "build": "npm-run-all -s *:build"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "0.12.2",
    "bs-platform": "6.2.1",
    "bundlesize": "0.18.0",
    "lebab": "3.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "rollup": "1.27.2",
    "rollup-plugin-terser": "5.1.2",
    "typescript": "3.7.2"
  },
  "bundlesize": [
    {
      "path": "javascript/*.js",
      "compression": "none"
    }
  ]
}
